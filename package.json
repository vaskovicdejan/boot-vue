{
  "name": "vite-boot",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.7",
  "description": "Vite starter template with UnoCSS",
  "keywords": [
    "viteBoot",
    "vite-boot",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.3",
    "@vueuse/core": "^10.2.1",
    "daisyui": "^3.2.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "unocss-preset-chinese": "^0.0.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@iconify-json/mdi": "^1.1.52",
    "@iconify-json/tabler": "^1.1.81",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@kirklin/eslint-config": "^0.4.1",
    "@types/node": "^20.4.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/compiler-dom": "^3.3.4",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "pnpm": "^8.6.7",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "unocss-preset-daisy": "^5.0.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.2",
    "vite-plugin-inspect": "^0.7.32",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-vue-devtools": "^0.5.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.4"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}

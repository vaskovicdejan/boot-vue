{
  "name": "boot-vue",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "description": "Vue starter template with UnoCSS",
  "keywords": [
    "BootVue",
    "boot-vue",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.4",
    "@vueuse/core": "^10.8.0",
    "daisyui": "^3.9.4",
    "iso-639-1": "^3.1.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.19",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@iconify-json/mdi": "^1.1.64",
    "@iconify-json/tabler": "^1.1.105",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@kirklin/eslint-config": "^2.1.0",
    "@types/node": "^20.11.20",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/compiler-dom": "^3.4.19",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.4",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unocss-preset-chinese": "^0.3.2",
    "unocss-preset-daisy": "^7.0.0",
    "unocss-preset-ease": "^0.0.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-pwa": "^0.19.0",
    "vite-plugin-vue-devtools": "^7.0.15",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}

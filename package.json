{
  "name": "vite-boot",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "description": "Vite starter template with TailwindCSS",
  "keywords": [
    "viteBoot",
    "vite-boot",
    "vite",
    "tailwindcss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.1",
    "@kirklin/reset-css": "^0.0.3",
    "@vueuse/core": "^9.13.0",
    "daisyui": "^2.50.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.30",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.15",
    "@iconify-json/mdi": "^1.1.47",
    "@iconify-json/tabler": "^1.1.61",
    "@intlify/unplugin-vue-i18n": "^0.8.2",
    "@kirklin/eslint-config": "^0.2.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.14.0",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/compiler-dom": "^3.2.47",
    "@vue/test-utils": "^2.3.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.27.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.14.3",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-macros": "^1.8.2",
    "vite": "^4.1.2",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-vue-inspector": "^3.3.2",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.4"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}

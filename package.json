{
  "name": "vite-boot",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "description": "Vite starter template with TailwindCSS",
  "keywords": [
    "viteBoot",
    "vite-boot",
    "vite",
    "tailwindcss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.3",
    "@vueuse/core": "^9.13.0",
    "daisyui": "^2.51.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/mdi": "^1.1.50",
    "@iconify-json/tabler": "^1.1.68",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@kirklin/eslint-config": "^0.3.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.15.11",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vue/compiler-dom": "^3.2.47",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "pnpm": "^8.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-icons": "^0.16.0",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^1.11.2",
    "vite": "^4.2.1",
    "vite-plugin-inspect": "^0.7.18",
    "vite-plugin-vue-inspector": "^3.4.0",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}

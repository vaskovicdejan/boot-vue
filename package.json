{
  "name": "boot-vue",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.0.1",
  "description": "Vue starter template with UnoCSS",
  "keywords": [
    "BootVue",
    "boot-vue",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.4",
    "@vueuse/core": "^10.9.0",
    "daisyui": "^3.9.4",
    "iso-639-1": "^3.1.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.23",
    "vue-i18n": "^9.12.1",
    "vue-router": "^4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/tabler": "^1.1.110",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@kirklin/eslint-config": "^2.3.1",
    "@types/node": "^20.12.7",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.59.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/compiler-dom": "^3.4.23",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^9.0.0",
    "eslint-plugin-format": "^0.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.0.1",
    "typescript": "^5.4.5",
    "unocss": "^0.59.3",
    "unocss-preset-chinese": "^0.3.2",
    "unocss-preset-daisy": "^7.0.0",
    "unocss-preset-ease": "^0.0.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.9",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-pwa": "^0.19.8",
    "vite-plugin-vue-devtools": "^7.0.27",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.13"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}

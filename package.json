{
  "name": "vite-boot",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "description": "Vite starter template with TailwindCSS",
  "keywords": [
    "viteBoot",
    "vite-boot",
    "vite",
    "tailwindcss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kidonng/daisyui": "^2.51.3",
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.3",
    "@vueuse/core": "^9.13.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "unocss-preset-chinese": "^0.0.6",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/mdi": "^1.1.49",
    "@iconify-json/tabler": "^1.1.65",
    "@intlify/unplugin-vue-i18n": "^0.9.1",
    "@kirklin/eslint-config": "^0.2.7",
    "@types/node": "^18.15.0",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/compiler-dom": "^3.2.47",
    "@vue/test-utils": "^2.3.1",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.29.1",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "unocss-preset-daisy": "^2.0.0",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}

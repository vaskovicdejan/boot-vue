{
  "name": "boot-vue",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "description": "Vue starter template with UnoCSS",
  "keywords": [
    "BootVue",
    "boot-vue",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.3",
    "@vueuse/core": "^10.5.0",
    "daisyui": "^3.9.4",
    "iso-639-1": "^3.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.7",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@iconify-json/mdi": "^1.1.55",
    "@iconify-json/tabler": "^1.1.96",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@kirklin/eslint-config": "^1.0.1",
    "@types/node": "^20.8.10",
    "@types/nprogress": "^0.2.2",
    "@unocss/eslint-config": "^0.57.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/compiler-dom": "^3.3.7",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "pnpm": "^8.10.2",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "unocss-preset-chinese": "^0.3.0",
    "unocss-preset-daisy": "^7.0.0",
    "unocss-preset-ease": "^0.0.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.3",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-inspect": "^0.7.41",
    "vite-plugin-pwa": "^0.16.7",
    "vite-plugin-vue-devtools": "^1.0.0-rc.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
